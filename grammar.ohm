DPLA {
  Expr = AddExpr | type

  type = number
  number = float | int
  int = digit+
  float = digit+ "." digit+

  AddExpr = number "+" number -- plus
  | number "-" number -- minus
  | MulExpr
  MulExpr = MulExpr "*" PriExpr -- times
  | MulExpr "/" PriExpr -- divide
  | MulExpr "%" PriExpr -- modulus
  | MulExpr "^" PriExpr -- power
  | PriExpr
  PriExpr = "(" AddExpr ")" -- paren
  | number

  symbol = (letter | "_") (letter | digit | "_")*
  Assignment = symbol "=" Expr
}